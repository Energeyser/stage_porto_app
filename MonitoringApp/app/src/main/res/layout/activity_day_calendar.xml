            android:layout_weight="1"
            android:orientation="vertical"
            android:clickable="true"
            android:onClick="openMonthSummary"
            android:layout_marginRight="1dp"
            android:background="#E0E0E0">
            <ImageView
                android:id="@+id/img_month_summary"
                android:layout_marginTop="7dp"
                android:layout_width="40dp"
                android:layout_height="30dp"
                android:layout_marginBottom="1dp"
                android:layout_gravity="center"
                android:src="@drawable/icon_summary"/>
            <TextView
                android:id="@+id/txt_month_summary"
                android:layout_width="40dp"
                android:layout_height="10dp"
                android:layout_marginBottom="2.5dp"
                android:layout_gravity="center"
                android:text="Summary"
                android:textAppearance="@style/TextMenu"/>
        </LinearLayout>
        <LinearLayout
            android:layout_height="match_parent"
            android:layout_width="0dp"
            android:layout_weight="1"
            android:orientation="vertical"
            android:clickable="true"
            android:onClick="openCalendar"
            android:layout_marginRight="1dp"
            android:background="#E0E0E0">
            <ImageView
                android:id="@+id/img_calendar_month_summary"
                android:layout_marginTop="7dp"
                android:layout_width="30dp"
                android:layout_height="30dp"
                android:layout_marginBottom="1dp"
                android:layout_gravity="center"
                android:src="@drawable/icon_calendar"/>
            <TextView
                android:id="@+id/txt_calendar_month_summary"
                android:layout_width="40dp"
                android:layout_height="10dp"
                android:layout_marginBottom="2.5dp"
                android:layout_gravity="center"
                android:text="Calendar"
                android:textAppearance="@style/TextMenu"/>
        </LinearLayout>
        <LinearLayout
            android:layout_height="match_parent"
            android:layout_width="0dp"
            android:layout_weight="1"
            android:orientation="vertical"
            android:clickable="true"
            android:onClick="openExport"
            android:layout_marginRight="1dp"
            android:background="#E0E0E0">
            <ImageView
                android:id="@+id/img_export_month_summary"
                android:layout_marginTop="7dp"
                android:layout_width="35dp"
                android:layout_height="30dp"
                android:layout_marginBottom="1dp"
                android:layout_gravity="center"
                android:src="@drawable/icon_export"/>
            <TextView
                android:id="@+id/txt_export_month_summary"
                android:layout_width="35dp"
                android:layout_height="10dp"
                android:layout_marginBottom="2.5dp"
                android:layout_gravity="center"
                android:text="Export"
                android:textAppearance="@style/TextMenu"/>
        </LinearLayout>
        <LinearLayout
            android:layout_height="match_parent"
            android:layout_width="0dp"
            android:layout_weight="1"
            android:orientation="vertical"
            android:clickable="true"
            android:onClick="openDoctorInformation"
            android:background="#E0E0E0">
            <ImageView
                android:id="@+id/img_doctor_month_summary"
                android:layout_marginTop="7dp"
                android:layout_width="35dp"
                android:layout_height="30dp"
                android:layout_marginBottom="1dp"
                android:layout_gravity="center"
                android:src="@drawable/icon_doctor"/>
            <TextView
                android:id="@+id/txt_doctor_month_summary"
                android:layout_width="35dp"
                android:layout_height="10dp"
                android:layout_marginBottom="2.5dp"
                android:layout_gravity="center"
                android:text="Doctor"
                android:textAppearance="@style/TextMenu"/>
        </LinearLayout>
    </LinearLayout>
</LinearLayout>

<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.monitoringapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".LandingActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MonthSummaryActivity" />
        <activity android:name=".CalendarActivity" />
        <activity android:name=".ExportActivity" />
        <activity android:name=".DoctorInformationActivity" />
        <activity android:name=".WeekCalendarActivity" />
        <activity android:name=".DayCalendarActivity" />
        <activity android:name=".DoctorModifyActivity" />
        <activity android:name=".ExportSuccessfulActivity" />
        <activity android:name=".PatientInformationsActivity" />
        <activity android:name=".PatientModifyActivity"></activity>
    </application>

</manifest>